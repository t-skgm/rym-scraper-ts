{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "start": {
      "cmd": "libs/main.ts",
      "watch": false,
      "allow": [
        "net",
        "write"
      ]
    },
    "test": {
      "cmd": "deno test ./ --watch",
      "allow": [
        "write",
        "read"
      ]
    },
    "test:update": "denon test -- --update"
  }
}
